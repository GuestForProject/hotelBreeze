{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "prisma": {
    "seed": "ts-node prisma/seeds.ts"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma && npm run prisma:generate",
    "prisma:migrate:reset": "prisma migrate reset -f --schema=./prisma/schema.prisma && npm run prisma:generate",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma && npm run prisma:generate",
    "postinstall": "npm run prisma:migrate:deploy && npm run prisma:generate"
  },
  "dependencies": {
    "@apollo/server": "4.9.3",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@nestjs/apollo": "12.0.3",
    "@nestjs/common": "10.2.6",
    "@nestjs/core": "10.2.6",
    "@nestjs/graphql": "12.0.3",
    "@nestjs/platform-express": "10.2.6",
    "@prisma/client": "^5.5.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "16.8.1",
    "graphql-subscriptions": "2.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@nestjs/cli": "10.1.18",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.6",
    "@types/express": "4.17.18",
    "@types/node": "20.8.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.6.4",
    "prisma": "^4.15.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-morph": "19.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
